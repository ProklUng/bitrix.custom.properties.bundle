{
    "name": "proklung/bitrix-custom-properties-bundle",
    "description": "Бандл для Битрикс + Symfony с набором кастомных свойств",
    "type": "symfony-bundle",
    "keywords": [
        "bitrix",
        "bitrix-symfony",
        "bitrix-bundle",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/bitrix-custom-properties-bundle/issues",
        "source": "https://github.com/ProklUng/bitrix-custom-properties-bundlee"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\BitrixCustomPropertiesBundle\\": ""
        }
    },
    "require": {
        "php": ">=7.1 | ~8",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "egulias/email-validator": "~2 || ~3",
        "illuminate/validation": "~6 | ~7 | ~8",
        "giggsey/libphonenumber-for-php": "^8.12"
    },
    "require-dev": {
        "proklung/bitrix-phpunit-testing-tools": "^1.4"
    },
    "extra": {
        "installer-paths": {
            "vendor/sheerockoff/bitrix-ci/files/bitrix/modules/{$name}/": ["type:bitrix-module"]
        }
    }
}
